<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->


    <style name="Theme.EndTerm" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <item name="android:windowActivityTransitions">true</item>


        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/MainTheme</item>
        <item name="colorPrimaryVariant">@color/lightPurple</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.EndTerm.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.Tamrin3.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.Tamrin3.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="Theme_Dialog" parent="ThemeOverlay.AppCompat.Dialog">
        <item name="android:windowMinWidthMajor">90%</item>
        <item name="android:windowMinWidthMinor">90%</item>
    </style>

</resources>